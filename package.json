{
  "name": "userjs-digger",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:script dev:style",
    "build": "vue-tsc --noEmit && npm run build:style && vite build",
    "preview": "vite preview",
    "dev:script": "vite",
    "dev:style": "unocss \"src/**/*.vue\" -o ./src/uno.css -w",
    "build:style": "unocss \"src/**/*.vue\" -o ./src/uno.css"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@unocss/cli": "^0.50.6",
    "@vitejs/plugin-vue": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.2",
    "unocss": "^0.50.6",
    "vite": "^4.2.1",
    "vite-plugin-monkey": "^3.0.3",
    "vue-tsc": "^1.2.0"
  }
}
